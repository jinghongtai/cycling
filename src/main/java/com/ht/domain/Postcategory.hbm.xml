<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.ht.domain.Postcategory" table="postcategory" schema="cycling">
        <id name="id">
            <column name="id" sql-type="varchar(36)" length="36"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="orderNo">
            <column name="orderNo" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>